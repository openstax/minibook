    <exercise id="fs-id1165134248021"><problem id="fs-id1165134248022"><para id="fs-id1165134248023"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
