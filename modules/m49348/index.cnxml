     <solution id="fs-id1165135517139"><para id="fs-id1165135517140"> <m:math>
 <m:mrow>
  <m:mn>3</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>11</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>34</m:mn><m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>106</m:mn>
   </m:mrow>
